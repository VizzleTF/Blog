{
  "headers": [
    {
      "source": "/mi.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/api/send",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "image/gif"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/mi.js",
      "destination": "https://uma.vaka.work/mi"
    },
    {
      "source": "/api/send",
      "destination": "https://uma.vaka.work/api/send"
    }
  ]
}